import random

random_numbers = [str]

# Generate random numbers and add to the list
while len(random_numbers) < 10:
    random_var = random.randint(1, 10)
    random_numbers.append(random_var)
    print(random_var)
    if random_var == 7:
        print("Found a 7, breaking out of the loop.")
        break

print(random_numbers)